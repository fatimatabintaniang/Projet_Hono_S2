{
  "openapi": "3.0.0",
  "info": {
    "title": "Gestion_Bibliotheque API",
    "version": "1.0.0",
    "description": "APP Bibliotheque + Auth avec JWT. Les routes * nécessitent l'en‑tête Authorization: Bearer <token>."
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Dev local"
    }
  ],
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "RegisterReq": {
        "type": "object",
        "required": [
          "nom",
          "prenom",
          "telephone",
          "image",
          "email",
          "password",
          "role"
        ],
        "properties": {
          "prenom": {
            "type": "string",
            "example": "Ada"
          },
          "nom": {
            "type": "string",
            "example": "Lovelace"
          },
          "telephone": {
            "type": "string",
            "example": "12 Rue des maths, Paris"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "ada@example.com"
          },
          "password": {
            "type": "string",
            "format": "password",
            "example": "secret123"
          },
          "image": {
            "type": "string",
            "format": "uri",
            "example": "https://res.cloudinary.com/...jpg"
          },
          "role": {
            "type": "string",
            "enum": [
              "ADMIN",
              "ENSEIGNANT",
              "ETUDIANT"
            ],
            "default": "ETUDIANT",
            "description": "Rôle de l'utilisateur (par défaut ETUDIANT)"
          }
        }
      },
      "LoginReq": {
        "type": "object",
        "required": [
          "email",
          "password"
        ],
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "example": "ada@example.com"
          },
          "password": {
            "type": "string",
            "format": "password",
            "example": "secret123"
          }
        }
      },
      "User": {
        "type": "object",
        "required": [
          "prenom",
          "nom",
          "adresse",
          "email"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "prenom": {
            "type": "string"
          },
          "nom": {
            "type": "string"
          },
          "adresse": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          }
        }
      },
      "TokenRes": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "example": "eyJhbGciOi..."
          }
        }
      },
      "Categorie": {
        "type": "object",
        "required": [
          "nom"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "nom": {
            "type": "string",
            "example": "Science Fiction"
          }
        }
      },
      "UpdateCategorie": {
        "type": "object",
        "required": [
          "nom"
        ],
        "properties": {
          "nom": {
            "type": "string",
            "example": "Science Fiction"
          }
        }
      },
      "Niveau": {
        "type": "object",
        "required": [
          "libelle"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "libelle": {
            "type": "string",
            "example": "Licence 1"
          }
        }
      },
      "UpdateNiveau": {
        "type": "object",
        "required": [
          "libelle"
        ],
        "properties": {
          "libelle": {
            "type": "string",
            "example": "Licence 1"
          }
        }
      },
      "Matiere": {
        "type": "object",
        "required": [
          "nom_matiere"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "nom_matiere": {
            "type": "string",
            "example": "Mathématiques"
          }
        }
      },
      "UpdateMatiere": {
        "type": "object",
        "required": [
          "nom_matiere"
        ],
        "properties": {
          "nom_matiere": {
            "type": "string",
            "example": "Mathématiques"
          }
        }
      },
      "Livre": {
        "type": "object",
        "required": [
          "titre",
          "auteur",
          "format",
          "chemin_fichier",
          "type",
          "categorieId",
          "niveauId",
          "matiereId"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "titre": {
            "type": "string",
            "example": "Introduction à la Programmation"
          },
          "auteur": {
            "type": "string",
            "example": "John Doe"
          },
          "format": {
            "type": "string",
            "example": "PDF"
          },
          "chemin_fichier": {
            "type": "string",
            "example": "/files/intro_programming.pdf"
          },
          "type": {
            "type": "string",
            "example": "Memoire"
          },
          "categorieId": {
            "type": "integer",
            "example": 2
          },
          "niveauId": {
            "type": "integer",
            "example": 3
          },
          "matiereId": {
            "type": "integer",
            "example": 4
          }
        }
      },
      "UpdateLivre": {
        "type": "object",
        "required": [
          "titre",
          "auteur",
          "format",
          "chemin_fichier",
          "type",
          "categorieId",
          "niveauId",
          "matiereId"
        ],
        "properties": {
          "titre": {
            "type": "string",
            "example": "Introduction à la Programmation"
          },
          "auteur": {
            "type": "string",
            "example": "John Doe"
          },
          "format": {
            "type": "string",
            "example": "PDF"
          },
          "chemin_fichier": {
            "type": "string",
            "example": "/files/intro_programming.pdf"
          },
          "type": {
            "type": "string",
            "example": "Memoire"
          },
          "categorieId": {
            "type": "integer",
            "example": 2
          },
          "niveauId": {
            "type": "integer",
            "example": 3
          },
          "matiereId": {
            "type": "integer",
            "example": 4
          }
        }
      },
      "LivreDownload": {
        "type": "object",
        "required": [
          "livreId",
          "userId",
          "date_telechargement"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "livreId": {
            "type": "integer",
            "example": 5
          },
          "userId": {
            "type": "integer",
            "example": 10
          },
          "date_telechargement": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T12:00:00Z"
          }
        }
      },
      "LivreFavori": {
        "type": "object",
        "required": [
          "livreId",
          "userId",
          "a_lire_plus_tard"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "livreId": {
            "type": "integer",
            "example": 5
          },
          "userId": {
            "type": "integer",
            "example": 10
          },
          "a_lire_plus_tard": {
            "type": "boolean",
            "example": true
          }
        }
      }
    },
    "parameters": {
      "Id": {
        "name": "id",
        "in": "path",
        "required": true,
        "schema": {
          "type": "integer",
          "example": 10
        }
      },
      "livreId": {
        "name": "id",
        "in": "path",
        "required": true,
        "schema": {
          "type": "integer",
          "example": 5
        }
      }
    }
  },
